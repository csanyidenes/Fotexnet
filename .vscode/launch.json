{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Listen for Xdebug",
        "type": "php",
        "request": "launch",
        "port": 9003,
        "pathMappings": {
          "/var/www/html": "${workspaceFolder}/src"
        },
        "xdebugSettings": {
            "max_depth": 5,
            "max_children": 1000,
            "show_hidden": 1,
            "max_data": 1000
        }
      }
    ]
  }
  